  MINIMUM_LATITUDE             = -4.714577079442 <DEG>
  LINE_PROJECTION_OFFSET       = -261165.50000001 <PIXEL>
  SAMPLE_PROJECTION_OFFSET     = 3738.5 <PIXEL>
  EASTERNMOST_LONGITUDE        = 137.45695095 <DEG>
  WESTERNMOST_LONGITUDE        = 137.33619755 <DEG>
